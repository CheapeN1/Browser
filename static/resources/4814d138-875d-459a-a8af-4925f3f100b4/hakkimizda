			<script>
				$(document).ready(function () {
					if ($.cookie('pop-cookie') == null) {
						$('#cookieUsagePopIn').css("display", "block");
						$.cookie('pop-cookie', '1');
					}
					$(".closeBtn").click(function () {
						$("#cookieUsagePopIn").hide();
					})
					$(".acceptAndClose").click(function () {
						$("#cookieUsagePopIn").hide();
					})
				});
